{
  "name": "unity-webgl",
  "version": "3.1.1",
  "description": "An easy solution for embedding Unity WebGL builds in your Vue.js or Web Application.",
  "keywords": [
    "unity",
    "webGL",
    "unity webGl",
    "vue unity",
    "vue unity webgl"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "types",
    "vue"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "default": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./vue": {
      "import": "./vue/index.js",
      "default": "./vue/index.js",
      "types": "./vue/index.d.ts"
    }
  },
  "homepage": "https://github.com/Meqn/UnityWebGL.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Meqn/UnityWebGL.js.git"
  },
  "author": "Mervin <mengqing723@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Meqn/UnityWebGL.js/issues"
  },
  "scripts": {
    "clean": "rimraf -rf ./dist && rimraf -rf ./vue",
    "dev": "NODE_ENV=development rollup -c rollup.config.js -w",
    "build": "npm run clean && NODE_ENV=production rollup -c rollup.config.js"
  },
  "devDependencies": {
    "@vue/composition-api": "^1.7.0",
    "vue": "^3.2.38"
  },
  "dependencies": {
    "vue-demi": "latest"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.7.0",
    "vue": "^2.5.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  }
}
